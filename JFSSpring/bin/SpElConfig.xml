<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
       <bean id="b1" class="MySpEl.Book1">
       <property name="bookId" value="100"></property>
       <property name="bookName" value="Fairy Tales"></property>
       </bean>
       <bean id="b2" class="MySpEl.Book2">
       <property name="book2Id" value="200"></property>
       <property name="book2Name" value="Harry Potter"></property>
       </bean>
       
       
</beans>